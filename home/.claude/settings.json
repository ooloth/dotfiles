{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(/dev/null)",
      "Bash(cat:*)",
      "Bash(chmod:*)",
      "Bash(echo:*)",
      "Bash(find:*)",
      "Bash(gh pr create:*)",
      "Bash(gh pr edit:*)",
      "Bash(gh pr view:*)",
      "Bash(git add:*)",
      "Bash(git branch:*)",
      "Bash(git checkout:*)",
      "Bash(git cherry-pick:*)",
      "Bash(git commit:*)",
      "Bash(git pull:*)",
      "Bash(git push:*)",
      "Bash(grep:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(timeout:*)",
      "Edit(*)",
      "MultiEdit(*)",
      "NotebookEdit(*)",
      "Write(*)"
    ],
    "deny": ["Bash(curl:*)", "Bash(git merge:*)", "Bash(git reset:*)"]
  }
}
